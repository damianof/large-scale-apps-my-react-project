{
  "name": "my-react-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode mock",
    "build": "tsc && vite build --mode production",
    "build-beta": "tsc && vite build --mode beta",
    "build-local": "tsc && vite build --mode localapis",
    "build-mock": "tsc && vite build --mode mock",
    "preview": "vite preview",
    "start": "npm run dev",
    "start-local": "vite --mode localapis",
    "test": "vitest run",
    "test-watch": "vitest wach",
    "test-coverage": "vitest run --coverage",
    "pretty": "prettier -w \"./src/**/*.*{ts,tsx,css,scss,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged \"./src/**/*.*{ts,vue,css,scss,html}\""
    }
  },
  "dependencies": {
    "@builtwithjavascript/formatters": "^1.0.5",
    "@reduxjs/toolkit": "^1.7.2",
    "axios": "^0.25.0",
    "i18next": "^21.6.11",
    "i18next-http-backend": "^1.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  }
}
